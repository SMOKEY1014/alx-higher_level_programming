#!/usr/bin/python3
import random

def print_last_digit(random_number):
    last_digit = random_number % 10
    print(f"The last digit of {random_number} is: {last_digit}")

# Generate a random number for demonstration
random_number = random.randint(1, 1000)

# Call the function with the random number
print_last_digit(random_number)
